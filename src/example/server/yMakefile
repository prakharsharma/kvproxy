	-I./ \
	-I/usr/local/include \
	-I/home/y/include \
	`pkg-config --cflags glib-2.0`

LLDLIBS = \
        -L/home/y/lib \
	      -Wl,-rpath,/home/y/lib \
    	-L/usr/local/lib \
	      -Wl,-rpath,/usr/local/lib \
    	-L../../lib \
	      -Wl,-rpath,../../lib \
        -lkvproxy \
    	-L./lib \
	      -Wl,-rpath,./lib \
        -lexampleHelpers \
        `pkg-config --libs glib-2.0 | sed 's/-liconv//'` \

