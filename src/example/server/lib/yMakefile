export PKG_CONFIG_PATH=/home/y/lib/pkgconfig
export BASE_DIR=./

ROOT=/home/y
include $(ROOT)/share/yahoo_cfg/Make.defs

CLEAN +=

ifeq ($(YMK_OS), FreeBSD)
OPTFLAG += -ftemplate-depth-50
endif

CXXSRC = \
         dbRecord.pb.cc \
         MessageUtils.cpp \

LIB_NAME = exampleHelpers
SHLIB_VERSION = 2

WARN += -Wall

LINC = \
	-I./ \
	-I/usr/local/include \
	-I/home/y/include \
	`pkg-config --cflags glib-2.0`

LLDLIBS = \
        -L/home/y/lib \
	      -Wl,-rpath,/home/y/lib \
    	-L/usr/local/lib \
	      -Wl,-rpath,/usr/local/lib \
    	-L../../../lib \
	      -Wl,-rpath,../../../lib \
        -lkvproxy \
        `pkg-config --libs glib-2.0 | sed 's/-liconv//'` \

include $(ROOT)/share/yahoo_cfg/Make.rules
# Do not place any rules or targets above this line

dbRecord.pb.cc: dbRecord.pb.h

dbRecord.pb.h: ./dbRecord.proto
	/home/y/bin/protoc --cpp_out=./ dbRecord.proto
